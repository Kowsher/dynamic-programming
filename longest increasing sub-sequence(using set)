#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int input[n+1];

    set < int > lis ;
    set < int > :: iterator  it ;

    for( int i = 0 ; i < n ; i++ )
    {
        cin>>input[i];
        lis.insert( input[i]);
        it = lis.find( input[i]);
        it++;
        if( it != lis.end()) lis.erase(it);
    }
    cout << lis.size() << endl ;

    for(auto& x: lis)cout<<x<<" ";


    return 0;
}
