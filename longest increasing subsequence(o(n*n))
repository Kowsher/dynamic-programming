#include<bits/stdc++.h>
using namespace std;  ///longest increasing subsequence with time compl: o(n*n) 

int main()
{
    int n;
    cin>>n;
    int arr[n+1], lis[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
        lis[i]=1;

    }


    for(int i=0; i<n; i++)
    {
        for(int j=0; j<=i; j++)
        {
            if(arr[j]<arr[i] )
            {
                if(lis[i]<lis[j]+1) lis[i]=lis[j]+1;

            }

        }
    }

    

    return 0;
}
