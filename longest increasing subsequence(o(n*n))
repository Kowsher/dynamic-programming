#include<bits/stdc++.h>
using namespace std;



int main()
{
    const int NX = 1000;

    int LIS[ NX + 5 ] , input[ NX + 5 ], n ;

    cin>>n;
    for( int i = 0 ; i < n ; i++ )
    {
        cin>>input[i];
    }
    int ans = 0 ;
    for( int i = 0 ; i < n ; i++ )
    {
        LIS[i] = 1 ;
        for( int j = 0; j < i ; j++ )
        {
            if( input[j] <= input[i] ) 
            {
                LIS[i] = max( LIS[i] , LIS[j] + 1 );
            }
        }
        ans = max( ans , LIS[i]);
    }
    cout<<ans<<endl;


    return 0;
    }
